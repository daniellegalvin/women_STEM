select(-diff) %>%
pivot_wider(id_cols=c(SD, .draw),
names_from=position,
values_from=c("female", "male")) %>%
pivot_wider(id_cols=c(.draw),
names_from=SD,
values_from=c("female_tt", "male_tt", "female_instructor", "male_instructor"))
